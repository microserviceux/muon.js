{
  "name": "muonjs",
  "version": "8.0.17-0",
  "description": "Muon, javascript edition",
  "main": "dist/MuonClient.js",
  "scripts": {
    "test": "jest",
    "dev": "node test/server/server.js",
    "build": "echo done"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/muoncore/muon.js.git"
  },
  "keywords": [
    "Muon",
    "microservices",
    "reactive",
    "streaming",
    "events",
    "browser"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "mapCoverage": true
  },
  "dependencies": {
    "@types/node": "^9.6.2",
    "@types/ws": "^4.0.2",
    "browser-websocket": "^2.2.0",
    "isomorphic-ws": "^4.0.1",
    "muon-core": "^7.3.0-79",
    "ts-node": "^7.0.1",
    "uuid": "^3.1.0",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/jest": "^23.3.1",
    "@types/sinon": "^4.3.1",
    "assert": "*",
    "async-rq": "*",
    "babel": "^6.5.2",
    "babel-core": "^6.4.5",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "*",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "*",
    "chai": "^4.1.2",
    "envify": "~1.2.1",
    "express": "^4.4.5",
    "jest": "^23.5.0",
    "jquery": "3.1.0",
    "json-markup": "1.0.0",
    "loose-envify": "*",
    "minifier": "*",
    "node-tcp-proxy": "^0.0.13",
    "rsvp": "*",
    "sinon": "^4.5.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1"
  },
  "author": "Muon Core Ltd",
  "license": "Apache2",
  "bugs": {
    "url": "https://github.com/muoncore/muon.js/issues"
  },
  "homepage": "https://github.com/muoncore/muon.js#readme"
}
